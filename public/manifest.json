{
  "name": "Two Tab",
  "version": "0.1.0",
  "description": "Saving tabs and sync between different devices",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Two Tab",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  }
}
